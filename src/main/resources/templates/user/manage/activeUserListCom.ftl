<#if (pager.list)?? >
    <#list  pager.list as e>
    <tr class="tr_body">
        <td>${(e.id)!''}</td>
        <td>${(e.userName)!''}</td>
        <td>${(e.phone)!''}</td>
        <td>${(e.weixin)!''}</td>    
        <td>
            <a  class="btn btn-xs btn-primary"  href="./activeUser?id=${(e.id)!''}" >查看详情</a>
        </td>
    </tr>
    </#list>
</#if>